@model TaskManagementSystem.Models.Task



<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- css link -->
    <!-- <link rel="stylesheet" href="./user.css"> -->
    @*<link rel="stylesheet" href="/css/layout.css">
        <link rel="stylesheet" href="/css/taskmangement.css">*@
    <link rel="stylesheet" href="/css/Adminlayout.css">
    <link rel="stylesheet" href="/css/Admintaskmgmt.css">
    <!-- font-awesome link -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta2/css/all.min.css" rel="stylesheet" />
    <title>Details</title>
</head>
<body>


    <div class="createtask-container-section">


        <!-- adduser section start here -->
        <div class="detail-form-section">
            <div class="detail-head-section">

                <h1>Details</h1>
                <hr>

            </div>

            <div class="detail-content-section">

                <div class="detail-section">
                    <h4 class="detail-title">
                        TaskName
                    </h4>
                    <h4>@Model.TaskName</h4>
                </div>

                <div class="detail-section">
                    <h4 class="detail-title">
                        TaskDetails
                    </h4>
                    <h4>@Model.TaskDetails</h4>
                </div>

                <div class="detail-section">
                    <h4 class="detail-title">
                        CreatedDate
                    </h4>
                    <h4>@Model.CreatedDate</h4>
                </div>


                <div class="detail-section">
                    <h4 class="detail-title">
                        AssignedDate
                    </h4>
                    <h4>@Model.AssignedDate</h4>
                </div>


                <div class="detail-section">
                    <h4 class="detail-title">
                        DueDate
                    </h4>
                    <h4>@Model.DueDate</h4>
                </div>

                <div class="detail-section">
                    <h4 class="detail-title">
                        AssignedBy
                    </h4>
                    <h4>@Model.AssignedBy.UserName</h4>
                </div>


                @*<div class="detail-section">
                    <h4 class="detail-title">
                        AssignedTo
                    </h4>
                    <h4>@Model.AssignedTo</h4>
                </div>

                <ul class="detail-section>
                    @foreach (var items in Model.AssignedTo)
                    {

            <li>@items</li>}
                </ul>*@



                <div class="detail-section">
                    <h4 class="detail-title">
                        TaskCategory
                    </h4>
                    <h4>@Model.TaskCategory.Name</h4>
                </div>


                <div class="detail-section">
                    <h4 class="detail-title">
                        TaskStatus
                    </h4>
                    <h4>@Model.TaskStatus</h4>
                </div>

                <div class="detail-section">
                    <h4 class="detail-title">
                        TaskCompletedDate
                    </h4>
                    <h4>@Model.TaskCompletedDate</h4>
                </div>


            </div>
           
            <div class="clearfix-btnn">
                <a  asp-controller="Task" asp-action="ViewTask" class="clearfix-btnn cancelbtnn">Cancel</a>
                @*<button class="clearfix-btn cancelbtn">Cancel</button>*@
            </div>

            


        </div>

        <script src="/js/adminlayout.js"></script>


</body>

















